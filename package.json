{
  "name": "axios-easy",
  "version": "0.1.6",
  "description": "A simple axios library",
  "type": "module",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "homepage": "https://github.com/GreatAuk/axios-easy",
  "bugs": {
    "url": "https://github.com/GreatAuk/axios-easy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreatAuk/axios-easy.git"
  },
  "author": "GreatAuk <greatauk11@gmail.com>",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./default-request-interceptor": {
      "types": "./dist/default-request-interceptor/index.d.ts",
      "import": "./dist/default-request-interceptor.js",
      "require": "./dist/default-request-interceptor.js"
    },
    "./default-response-interceptor": {
      "types": "./dist/default-response-interceptor/index.d.ts",
      "import": "./dist/default-response-interceptor.js",
      "require": "./dist/default-response-interceptor.js"
    },
    "./error-message-interceptor": {
      "types": "./dist/error-message-interceptor/index.d.ts",
      "import": "./dist/error-message-interceptor.js",
      "require": "./dist/error-message-interceptor.js"
    },
    "./authenticate-interceptor": {
      "types": "./dist/authenticate-interceptor/index.d.ts",
      "import": "./dist/authenticate-interceptor.js",
      "require": "./dist/authenticate-interceptor.js"
    },
    "./params-serializer-interceptor": {
      "types": "./dist/params-serializer-interceptor/index.d.ts",
      "import": "./dist/params-serializer-interceptor.js",
      "require": "./dist/params-serializer-interceptor.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.js"
    },
    "./openapi-ts-request-util": {
      "types": "./dist/openapi-ts-request-util/index.d.ts",
      "import": "./dist/openapi-ts-request-util.js",
      "require": "./dist/openapi-ts-request-util.js"
    },
    "./extend-axios-type": {
      "types": "./dist/extend-axios-type.d.ts",
      "import": "./dist/extend-axios-type.js",
      "require": "./dist/extend-axios-type.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stale-dep && tsdown",
    "dev": "stale-dep && tsdown --watch",
    "test": "stale-dep && vitest",
    "test:once": "stale-dep && vitest run",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "lint": "oxlint --fix",
    "postinstall": "stale-dep -u"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.15.17",
    "@types/qs": "^6.14.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axios-mock-adapter": "^2.1.0",
    "bumpp": "^10.1.0",
    "oxlint": "^1.12.0",
    "stale-dep": "^0.8.3",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": "22.16.0",
    "pnpm": "10.15.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "file-saver": "^2.0.5",
    "qs": "^6.14.0"
  }
}